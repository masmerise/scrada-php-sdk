{
    "name": "masmerise/scrada-php-sdk",
    "description": "An unofficial PHP SDK for accessing Scrada's API.",
    "license": "MIT",
    "keywords": [
        "scrada",
        "api",
        "sdk"
    ],
    "authors": [
        {
            "name": "Muhammed Sari",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/masmerise/scrada-php-sdk",
    "require": {
        "php": "~8.4",
        "psr/simple-cache": "^3.0",
        "saloonphp/rate-limit-plugin": "^2.1",
        "saloonphp/saloon": "^3.14",
        "webmozart/assert": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "phpstan/phpstan": "^2.1.31",
        "phpunit/phpunit": "^12.4.1",
        "vlucas/phpdotenv": "^5.6.2"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Scrada\\": "src"
        },
        "files": [
            "src/Core/Utility/transform.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "stan": "vendor/bin/phpstan analyse --memory-limit=2G",
        "test": "vendor/bin/phpunit",
        "verify": ["@stan", "@test"]
    }
}
